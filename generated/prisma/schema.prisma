generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int       @id @default(autoincrement())
  email    String    @unique
  password String
  products Product[]
}

model Product {
  id          Int    @id @default(autoincrement())
  name        String
  description String
  price       Float
  user        User   @relation(fields: [userId], references: [id])
  userId      Int
}
